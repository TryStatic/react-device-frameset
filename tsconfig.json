{
  "compilerOptions": {
    "target": "ES5",
    "baseUrl": ".",
    "jsx": "react",
    "lib": ["ES2015", "DOM"],
    "moduleResolution": "Node",
    "esModuleInterop": true,
    "strict": true,
    "strictNullChecks": true,
    "noImplicitAny": true,
    "strictFunctionTypes": true,
    "strictBindCallApply": true,
    "strictPropertyInitialization": true,
    "alwaysStrict": true,
    "paths": {
      "src/*": ["./src/*"]
    }
  }
}
